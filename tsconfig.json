{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es2015", "es2017", "es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2017",
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
    "strict": true,
    "sourceMap": true,
    "outDir": "./dist",
    "declaration": false,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "*": ["src/@types/*", "node_modules/@types/*"]
    }
  }
}
